            <div class="honorary container">
              <div class="left arrow" ><i class="fa fa-chevron-left 6x"></i></div>
              <div class="right arrow"><i class="fa fa-chevron-right 6x"></i></div>

              <div style="width: 100%">
                <h4 class="subheading">Meet the Honorary Board<hr size="2" color="#3E3F4"></h4>
              </div>

              <div class="inner-container">
                <% var panels = Math.ceil(board.length/4); %>
                <div class="content-container" style="width: <%= panels *100 %>%;)">
                  <% for (var i = 0; i < board.length; i++) { %>
                  <div class="three columns board">

                    <div class="boardwindow">
                      <div class="innerwindow">
                        <img src="<%= board[i].image %>" alt="<%= board[i].name %>"/>
                      </div>
                    </div>
                    <p><%= board[i].name %></p>
                  </div>
                  <% } %>

                </div>
              </div>
            </div>

            <script type="text/javascript">
              var currentpanel = 0;
              $('.right').click(function(){
                if(currentpanel < <%= panels - 1 %>){
                  var scroll = $('.inner-container').width();
                  scroll += currentpanel*scroll;
                  currentpanel++;
                  $('.board').each(function() {
                    $(this).animate({
                     left: scroll * -1
                   }, 500);
                    if(currentpanel==<%= panels - 1 %>){
                      $('.left').fadeIn(0,'linear');
                      $('.right').fadeOut(0,'linear');
                    }else{
                      $('.left').fadeIn(0,'linear');
                      $('.right').fadeIn(0,'linear');
                    }
                  })
                }
              });

              $('.left').click(function(){
               if(currentpanel > 0 ){
                var scroll = $('.inner-container').width();
                scroll -= currentpanel*scroll;
                currentpanel--;
                $('.board').each(function() {
                  $(this).animate({
                   left: scroll
                 }, 500);
                  if(currentpanel==0){
                    $('.left').fadeOut(0,'linear');
                    $('.right').fadeIn(0,'linear');
                  }else{
                    $('.left').fadeIn(0,'linear');
                    $('.right').fadeIn(0,'linear');
                  }
                })
              }
            });
              $( document ).ready(function() {
                $('.left').fadeOut(0,'linear');
              });



            </script>



