      <script type="text/javascript">
        $(document).ready(function(){

          $('.slider').slick({
            dots: false,
            infinite: true,
            speed: 300,
            slidesToShow: 1,
            slidesToScroll: 1,
            autoplay: true,
            autoplaySpeed: 5000,
            lazyLoad: 'progressive',
            onBeforeChange: function(){
              var newQuote = false;
              if($('.slider').slickCurrentSlide()==1){
                newQuote=true;
              }
              $('.heroblurb').fadeOut(100,function(){
                if(newQuote){
                  randomQuote();
                }
              });
            },
            onAfterChange: function(){
              $('.heroblurb').fadeIn(500,function(){});
            },

            responsive: [{
              breakpoint: 768,
              settings: {
                onBeforeChange: function(){
                  $('.heroblurb').fadeOut(100,function(){
                   quote(2);
                 });

                },
                onAfterChange: function(){
                  $('.mobileblurb').fadeIn(500,function(){});
                }
              }
            },
            {
              breakpoint: 480,
              settings: {
                onBeforeChange: function(){
                  $('.heroblurb').fadeOut(100,function(){
                  });

                },
                onAfterChange: function(){
                  $('.portraitblurb').fadeIn(500,function(){});
                }
              }
            }]

          });
});

var qs = [];
<% for (var i=0; i<quotes.length; i++){ %>
  qs[<%= i %>] = "<%= quotes[i].quote %>";
  <% } %>

  var qas = [];
  <% for (var i=0; i<quotes.length; i++){ %>
    qas[<%= i %>] = "<%= quotes[i].author %>";
    <% } %>

    var randprime = -1
    var randomQuote = function(){

      var rand = Math.floor(Math.random() * qs.length);
      while(rand==randprime){
        rand = Math.floor(Math.random() * qs.length);
      }
      $('.randomquote').html('"' + qs[rand] + '"');
      $('.randomquoteauthor').html("- " + qas[rand]);
      randprime = rand;
    }

    var quote = function(index){
      $('.randomquote').html('"' + qs[index] + '"');
      $('.randomquoteauthor').html("- " + qas[index]);
    }

    $( document ).ready(function() {
     randomQuote();
   });
  </script>







