    <script type="text/javascript">
      var current = 0;
      for (var i =1; i < $('.sponsor').length;i++){
        $('.sponsor').eq(i).fadeOut(0);
      }

      var nextSponsor = function(){
        $('.sponsor').eq(current).fadeOut(500, function(){
          current = (current +1) % $('.sponsor').length
