              <div style="width: 100%">
                <h4 class="subheading">Meet the Honorary Board<hr size="3" color="#3E3F4"></h4>
              </div>

              <div class="inner-container">
                <% var panels = Math.ceil(board.length/4); %>
                <div class="content-container" style="width: <%= panels *100 %>%;)">
                  <% for (var i = 0; i < board.length; i++) { %>
                  <div class="three columns board">
                    <div class="stretchy-wrapper">
                      <img src="<%= board[i] %>"/>
                    </div>
                  </div>
                  <% } %>

                </div>
              </div>
            </div>

            <script type="text/javascript">
              var currentpanel = 0;
              $('.right').click(function(){
                if(currentpanel < <%= panels - 1 %>){
                  var scroll = $('.inner-container').width();
                  scroll += currentpanel*scroll;
                  currentpanel++;
                  $('.board').each(function() {
                    $(this).animate({
                     left: scroll * -1
                   }, 500);
                  })
                }
              });

              $('.left').click(function(){
               if(currentpanel > 0 ){
                var scroll = $('.inner-container').width();
                scroll -= currentpanel*scroll;
                currentpanel--;
                $('.board').each(function() {
                  $(this).animate({
                   left: scroll
                 }, 500);
                })
              }
            });


            </script>




